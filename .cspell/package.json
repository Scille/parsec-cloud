{
  "name": "cspell-cfg",
  "version": "1.0.0",
  "description": "Parsec cspell configuration package",
  "scripts": {
    "lint": "cspell lint --config=cspell.config.yml --no-progress --dot --gitignore",
    "check": "cspell check --config=cspell.config.yml",
    "trace": "cspell trace --config=cspell.config.yml",
    "suggestions": "cspell suggestions --config=cspell.config.yml"
  },
  "dependencies": {
    "@cspell/dict-fr-fr": "^2.3.1",
    "@cspell/dict-fr-reforme": "^2.3.1",
    "@cspell/dict-win32": "^2.0.8",
    "cspell": "^9.1.5"
  }
}
