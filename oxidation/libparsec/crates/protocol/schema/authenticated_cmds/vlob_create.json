                    "type": "SequesterServiceID"
                },
                {
                    "name": "service_label",
                    "type": "String"
                },
                {
                    "name": "service_error",
                    "type": "String"
                }
            ]
        },
        {
            "status": "sequester_webhook_failed",
            "other_fields": [{
