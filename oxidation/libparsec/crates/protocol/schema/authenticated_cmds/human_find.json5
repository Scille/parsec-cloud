[
    {
        "label": "HumanFind",
        "major_versions": [
            2,
            3
        ],
        "req": {
            "cmd": "human_find",
            "fields": {
                "query": {
                    "type": "RequiredOption<String>"
                },
                "omit_revoked": {
                    "type": "Boolean"
                },
                "omit_non_human": {
                    "type": "Boolean"
                },
                // First page is 1
                "page": {
                    "type": "NonZeroInteger"
                },
                "per_page": {
                    "type": "IntegerBetween1And100"
                }
            }
        },
        "reps": {
            "ok": {
                "fields": {
                    "results": {
                        "type": "List<HumanFindResultItem>"
                    },
                    "page": {
                        "type": "NonZeroInteger"
                    },
                    "per_page": {
                        "type": "IntegerBetween1And100"
                    },
                    "total": {
                        "type": "Size"
                    }
                }
            },
            "not_allowed": {
                "fields": {
                    "reason": {
                        "type": "NonRequiredOption<String>"
                    }
                }
            }
        },
        "nested_types": {
            "HumanFindResultItem": {
                "fields": {
                    "user_id": {
                        "type": "UserID"
                    },
                    "human_handle": {
                        "type": "RequiredOption<HumanHandle>"
                    },
                    "revoked": {
                        "type": "Boolean"
                    }
                }
            }
        }
    }
]
