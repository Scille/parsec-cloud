{
  "name": "client",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "postinstall": "cd electron && npm install",
    "android:copy": "npx ionic build && npx cap copy android",
    "ios:copy": "npx ionic build && npx cap copy ios",
    "electron:copy": "npx ionic build && npx cap copy @capacitor-community/electron && cd electron && npm run libparsec",
    "android:open": "npm run android:copy && npx cap open android",
    "ios:open": "npm run ios:copy && npx cap open ios",
    "electron:open": "npm run electron:copy && npx cap open @capacitor-community/electron",
    "electron:dist": "npx ionic build && npx cap copy @capacitor-community/electron && cd electron && npm run electron:dist"

  },
  "dependencies": {
    "@capacitor-community/electron": "^4.1.0",
    "@capacitor/android": "3.4.3",
    "@capacitor/app": "1.1.1",
    "@capacitor/core": "3.4.3",
    "@capacitor/haptics": "1.1.4",
    "@capacitor/ios": "3.4.3",
    "@capacitor/keyboard": "1.2.2",
    "@capacitor/splash-screen": "^1.2.2",
    "@capacitor/status-bar": "1.0.8",
    "@ionic/storage": "^3.0.6",
    "@ionic/vue": "^6.0.13",
    "@ionic/vue-router": "^6.0.13",
    "core-js": "^3.6.5",
    "ionicons": "^6.0.1",
    "vue": "^3.2.31",
    "vue-i18n": "^9.2.0-beta.33",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@capacitor/cli": "3.4.3",
    "@intlify/eslint-plugin-vue-i18n": "^1.4.0",
    "@ionic/cli": "^6.0.0",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "@vue/cli-plugin-babel": "~5.0.1",
    "@vue/cli-plugin-e2e-cypress": "~5.0.1",
    "@vue/cli-plugin-eslint": "~5.0.1",
    "@vue/cli-plugin-router": "~5.0.1",
    "@vue/cli-plugin-typescript": "~5.0.1",
    "@vue/cli-plugin-unit-jest": "~5.0.1",
    "@vue/cli-service": "5.0.1",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^2.0.0-rc.16",
    "@vue/vue3-jest": "^27.0.0-alpha.3",
    "babel-jest": "^27.3.1",
    "cypress": "^8.7.0",
    "eslint": "^8.4.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^8.2.0",
    "jest": "^27.3.1",
    "native-run": "^1.5.0",
    "sass": "^1.49.9",
    "sass-loader": "^10.2.1",
    "ts-jest": "^27.0.7",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": ">=16.13.0 <19.0.0"
  },
  "description": "An Ionic project"
}
