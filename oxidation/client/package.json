{
    "name": "client",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "postinstall": "cd electron && npm install",
        "test": "npm run test:unit && npm run test:e2e:headless",
        "test:unit": "vue-cli-service test:unit",
        "test:e2e": "vue-cli-service test:e2e",
        "test:e2e:headless": "vue-cli-service test:e2e --headless",
        "lint": "vue-cli-service lint",
        "native:build": "vue-cli-service build",
        "android:copy": "npm run native:build && npx cap copy android",
        "android:copy:release": "npm run native:build && npx cap copy android",
        "android:open": "npm run android:copy && npx cap open android",
        "ios:copy": "npm run native:build && npx cap copy ios",
        "ios:copy:release": "npm run native:build && npx cap copy ios",
        "ios:open": "npm run ios:copy && npx cap open ios",
        "electron:copy": "npm run native:build && npx cap copy @capacitor-community/electron",
        "electron:copy:release": "npm run native:build && npx cap copy @capacitor-community/electron",
        "electron:open": "npm run electron:copy && npx cap open @capacitor-community/electron",
        "electron:release": "npm run electron:copy:release && cd electron && npm run electron:release",
        "web:open": "vue-cli-service serve",
        "web:release": "vue-cli-service build",
        "web:only-libparsec-plugin": "cd ../bindings/web && wasm-pack build --out-name index --out-dir pkg"
    },
    "dependencies": {
        "@capacitor-community/electron": "^4.1.0",
        "@capacitor/android": "3.4.3",
        "@capacitor/app": "1.1.1",
        "@capacitor/core": "3.9.0",
        "@capacitor/haptics": "1.1.4",
        "@capacitor/ios": "3.9.0",
        "@capacitor/keyboard": "1.2.2",
        "@capacitor/splash-screen": "^1.2.2",
        "@capacitor/status-bar": "1.0.8",
        "@ionic/storage": "^3.0.6",
        "@ionic/vue": "^6.2.0",
        "@ionic/vue-router": "^6.2.0",
        "core-js": "^3.22.7",
        "ionicons": "^6.0.4",
        "vue": "^3.2.45",
        "vue-i18n": "^9.2.2",
        "vue-router": "^4.1.6"
    },
    "devDependencies": {
        "@capacitor/cli": "4.6.1",
        "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
        "@ionic/cli": "^6.20.6",
        "@types/jest": "^27.0.2",
        "@types/node": "^18.0.1",
        "@typescript-eslint/eslint-plugin": "^5.47.1",
        "@typescript-eslint/parser": "^5.27.0",
        "@vue/cli-plugin-babel": "~5.0.8",
        "@vue/cli-plugin-e2e-cypress": "~5.0.4",
        "@vue/cli-plugin-eslint": "~5.0.8",
        "@vue/cli-plugin-router": "~5.0.8",
        "@vue/cli-plugin-typescript": "~5.0.8",
        "@vue/cli-plugin-unit-jest": "~5.0.8",
        "@vue/cli-service": "5.0.4",
        "@vue/eslint-config-typescript": "^10.0.0",
        "@vue/test-utils": "^2.2.6",
        "@vue/vue3-jest": "^27.0.0-alpha.3",
        "@wasm-tool/wasm-pack-plugin": "^1.6.0",
        "babel-jest": "^27.3.1",
        "cypress": "^11.0.0",
        "eslint": "^8.16.0",
        "eslint-plugin-cypress": "^2.12.1",
        "eslint-plugin-vue": "^8.2.0",
        "jest": "^27.3.1",
        "native-run": "^1.5.0",
        "sass": "^1.49.9",
        "sass-loader": "^13.2.0",
        "ts-jest": "^27.0.7",
        "typescript": "^4.9.4",
        "wasm-pack": "^0.10.3"
    },
    "engines": {
        "node": ">=16.13.0 <19.0.0"
    },
    "description": "An Ionic project"
}
