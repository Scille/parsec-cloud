{
    "name": "client",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "postinstall": "cd electron && npm install",
        "test:unit": "vue-cli-service test:unit",
        "test:e2e": "vue-cli-service test:e2e",
        "lint": "vue-cli-service lint",
        "build": "npx ionic build",
        "android:copy": "npm run build && npx cap copy android",
        "android:copy:release": "npm run build -- --prod && npx cap copy android",
        "android:open": "npm run android:copy && npx cap open android",
        "ios:copy": "npm run build && npx cap copy ios",
        "ios:copy:release": "npm run build -- --prod && npx cap copy ios",
        "ios:open": "npm run ios:copy && npx cap open ios",
        "electron:copy": "npm run build && npx cap copy @capacitor-community/electron",
        "electron:copy:release": "npm run build -- --prod && npx cap copy @capacitor-community/electron",
        "electron:open": "npm run electron:copy && npx cap open @capacitor-community/electron",
        "electron:release": "npm run electron:copy:release && cd electron && npm run electron:release",
        "web:libparsec:build": "cd ../bindings/web && wasm-pack build --dev --target web",
        "web:libparsec:build:release": "cd ../bindings/web && wasm-pack build --release --target web",
        "web:libparsec:copy": "cp -r ../bindings/web/pkg . || xcopy ..\\bindings\\web\\pkg pkg /e /s /y /i",
        "web:copy": "npm run web:libparsec:build && npm run web:libparsec:copy",
        "web:copy:release": "npm run web:libparsec:build:release && npm run web:libparsec:copy",
        "web:open": "npm run web:copy && vue-cli-service serve",
        "web:release": "npm run web:copy:release && vue-cli-service build"
    },
    "dependencies": {
        "@capacitor-community/electron": "^4.1.0",
        "@capacitor/android": "3.4.3",
        "@capacitor/app": "1.1.1",
        "@capacitor/core": "3.9.0",
        "@capacitor/haptics": "1.1.4",
        "@capacitor/ios": "3.4.3",
        "@capacitor/keyboard": "1.2.2",
        "@capacitor/splash-screen": "^1.2.2",
        "@capacitor/status-bar": "1.0.8",
        "@ionic/storage": "^3.0.6",
        "@ionic/vue": "^6.2.0",
        "@ionic/vue-router": "^6.2.0",
        "core-js": "^3.22.7",
        "ionicons": "^6.0.4",
        "vue": "^3.2.40",
        "vue-i18n": "^9.2.0-beta.35",
        "vue-router": "^4.1.5"
    },
    "devDependencies": {
        "@capacitor/cli": "4.6.0",
        "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
        "@ionic/cli": "^6.20.3",
        "@types/jest": "^27.0.2",
        "@types/node": "^18.0.1",
        "@typescript-eslint/eslint-plugin": "^5.6.0",
        "@typescript-eslint/parser": "^5.27.0",
        "@vue/cli-plugin-babel": "~5.0.4",
        "@vue/cli-plugin-e2e-cypress": "~5.0.4",
        "@vue/cli-plugin-eslint": "~5.0.1",
        "@vue/cli-plugin-router": "~5.0.1",
        "@vue/cli-plugin-typescript": "~5.0.8",
        "@vue/cli-plugin-unit-jest": "~5.0.8",
        "@vue/cli-service": "5.0.4",
        "@vue/eslint-config-typescript": "^10.0.0",
        "@vue/test-utils": "^2.0.0",
        "@vue/vue3-jest": "^27.0.0-alpha.3",
        "babel-jest": "^27.3.1",
        "cypress": "^10.9.0",
        "eslint": "^8.16.0",
        "eslint-plugin-cypress": "^2.12.1",
        "eslint-plugin-vue": "^8.2.0",
        "jest": "^27.3.1",
        "native-run": "^1.5.0",
        "sass": "^1.49.9",
        "sass-loader": "^13.2.0",
        "ts-jest": "^27.0.7",
        "typescript": "^4.3.5",
        "wasm-pack": "^0.10.3"
    },
    "engines": {
        "node": ">=16.13.0 <19.0.0"
    },
    "description": "An Ionic project"
}
