{
  "name": "client",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "android:copy": "npx ionic build && npx cap copy android",
    "ios:copy": "npx ionic build && npx cap copy ios",
    "electron:copy": "npx ionic build && npx cap copy @capacitor-community/electron && cd electron && npm run libparsec",
    "android:open": "npm run android:copy && npx cap open android",
    "ios:open": "npm run ios:copy && npx cap open ios",
    "electron:open": "npm run electron:copy && npx cap open @capacitor-community/electron"
  },
  "dependencies": {
    "@capacitor-community/electron": "^4.1.0",
    "@capacitor/android": "3.4.3",
