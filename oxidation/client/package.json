{
  "name": "client-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "postinstall": "cd electron && npm install",
    "dev": "vite",
    "preview": "vite preview",
    "lint": "eslint . && vue-tsc --noEmit",
    "test:unit": "cypress run --component",
    "test:e2e": "cypress run --e2e",
    "web:open": "vite",
    "web:release": "vite build && run lint",
    "native:build": "node ./scripts/vite_build_for_native.cjs",
    "android:copy": "npm run native:build && npx cap copy android",
    "android:copy:release": "npm run native:build && npx cap copy android",
    "android:open": "npm run android:copy && npx cap open android",
    "ios:copy": "npm run native:build && npx cap copy ios",
    "ios:copy:release": "npm run native:build && npx cap copy ios",
    "ios:open": "npm run ios:copy && npx cap open ios",
    "electron:copy": "npm run native:build && npx cap copy @capacitor-community/electron",
    "electron:open": "npm run electron:copy && npx cap open @capacitor-community/electron",
    "electron:release": "npm run electron:copy && cd electron && npm run electron:release"
  },
  "dependencies": {
    "@capacitor-community/electron": "^4.1.2",
    "@capacitor/android": "^4.7.1",
    "@capacitor/app": "^4.1.1",
    "@capacitor/core": "^4.7.1",
    "@capacitor/haptics": "^4.1.0",
    "@capacitor/ios": "^4.7.1",
    "@capacitor/keyboard": "^4.1.1",
    "@capacitor/splash-screen": "^4.2.0",
    "@capacitor/status-bar": "^4.1.1",
    "@ionic/storage": "^3.0.6",
    "@ionic/vue": "^6.6.0",
    "@ionic/vue-router": "^6.6.0",
    "@types/luxon": "^3.2.0",
    "luxon": "^3.3.0",
    "vite-plugin-top-level-await": "^1.3.0",
    "vue": "^3.2.45",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@capacitor/cli": "^4.7.1",
    "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
    "@ionic/cli": "^6.20.9",
    "@types/luxon": "^3.2.0",
    "@types/node": "^18.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.3.0",
    "cypress": "^12.7.0",
    "cypress-vite": "^1.3.1",
    "eslint": "^8.36.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^8.0.1",
    "happy-dom": "^9.8.1",
    "sass": "^1.58.3",
    "sass-loader": "^13.2.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.0",
    "vitest": "^0.29.2",
    "vue-tsc": "^1.0.24"
  }
}
