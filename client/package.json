{
    "name": "client-vite",
    "private": true,
    "version": "3.1.1-a.0+dev",
    "type": "module",
    "license": "BUSL-1.1",
    "scripts": {
        "postinstall": "cd electron && npm install",
        "dev": "vite",
        "preview": "vite preview",
        "lint": "eslint . --max-warnings=0 && vue-tsc --noEmit",
        "test:unit": "vitest run --dom",
        "test:e2e": "npx playwright test --headed --reporter=null --timeout=30000",
        "test:e2e:headless": "npx playwright test --reporter=null --timeout=30000",
        "web:open": "vite --open",
        "web:release": "npm run lint && vite build",
        "native:build": "node ./scripts/vite_build_for_native.cjs",
        "android:copy": "npm run native:build && npx cap copy android",
        "android:copy:release": "npm run native:build && npx cap copy android",
        "android:open": "npm run android:copy && npx cap open android",
        "ios:copy": "npm run native:build && npx cap copy ios",
        "ios:copy:release": "npm run native:build && npx cap copy ios",
        "ios:open": "npm run ios:copy && npx cap open ios",
        "electron:copy": "npm run native:build && npx cap copy @capacitor-community/electron",
        "electron:copy:dev": "npm run native:build -- --mode development && npx cap copy @capacitor-community/electron",
        "electron:open": "npm run electron:copy:dev && npx cap open @capacitor-community/electron",
        "electron:release": "npm run electron:copy && cd electron && npm run electron:release",
        "electron:release:dev": "npm run electron:copy:dev && cd electron && npm run electron:release",
        "electron:debug": "npm run electron:copy && cd electron && npm run electron:start",
        "prettier:check": "prettier . --check",
        "prettier:fix": "prettier . --write",
        "megashark:local:start": "npm link megashark-lib",
        "megashark:local:stop": "npm unlink megashark-lib --no-save && npm install"
    },
    "dependencies": {
        "@capacitor-community/electron": "^5.0.1",
        "@capacitor/android": "^5.6.0",
        "@capacitor/app": "^5.0.6",
        "@capacitor/core": "^5.2.1",
        "@capacitor/haptics": "^5.0.6",
        "@capacitor/ios": "^5.6.0",
        "@capacitor/keyboard": "^5.0.7",
        "@capacitor/splash-screen": "^5.0.6",
        "@capacitor/status-bar": "^5.0.6",
        "@ionic/storage": "^4.0.0",
        "@ionic/vue": "^7.6.4",
        "@ionic/vue-router": "^7.6.4",
        "@sentry/vue": "^8.38.0",
        "@types/luxon": "^3.3.4",
        "axios": "^1.7.7",
        "file-type": "^19.6.0",
        "luxon": "^3.5.0",
        "megashark-lib": "git+https://github.com/Scille/megashark-lib.git#a9430d2cd6d0f481ff4fef3c53682553a1391822",
        "qrcode-vue3": "^1.7.1",
        "uuid": "^11.0.3",
        "vue": "^3.5.13",
        "vue-i18n": "^10.0.4",
        "vue-router": "^4.4.5"
    },
    "devDependencies": {
        "@capacitor/cli": "^5.6.0",
        "@intlify/eslint-plugin-vue-i18n": "^3.0.0",
        "@ionic/cli": "^7.2.0",
        "@playwright/test": "^1.48.2",
        "@sentry/vite-plugin": "^2.22.6",
        "@types/luxon": "^3.3.4",
        "@types/node": "^22.9.0",
        "@types/uuid": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^8.14.0",
        "@typescript-eslint/parser": "^8.14.0",
        "@vitejs/plugin-vue": "^5.2.0",
        "@vue/eslint-config-typescript": "^14.1.3",
        "@vue/test-utils": "^2.4.6",
        "@vue/typescript": "^1.8.20",
        "@zxcvbn-ts/core": "^3.0.4",
        "@zxcvbn-ts/language-common": "^3.0.4",
        "@zxcvbn-ts/language-en": "^3.0.2",
        "@zxcvbn-ts/language-fr": "^3.0.2",
        "dotenv": "^16.4.5",
        "esbuild": "^0.24.0",
        "eslint": "^9.15.0",
        "eslint-config-standard-with-typescript": "^43.0.1",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-n": "^17.13.2",
        "eslint-plugin-no-relative-import-paths": "^1.5.5",
        "eslint-plugin-promise": "^7.1.0",
        "eslint-plugin-vue": "^9.18.1",
        "eslint-plugin-vue-scoped-css": "^2.8.1",
        "happy-dom": "^15.11.6",
        "prettier": "3.3.3",
        "prettier-plugin-organize-imports": "^4.1.0",
        "sass": "^1.81.0",
        "sass-loader": "^16.0.3",
        "start-server-and-test": "^2.0.8",
        "typescript": "^5.6.3",
        "unique-names-generator": "^4.7.1",
        "vite": "^5.4.11",
        "vite-plugin-top-level-await": "^1.4.4",
        "vitest": "^2.1.5",
        "vue-tsc": "^2.1.10"
    }
}
