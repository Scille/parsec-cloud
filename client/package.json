{
    "name": "client-vite",
    "private": true,
    "version": "3.7.2-a.0+dev",
    "type": "module",
    "license": "BUSL-1.1",
    "scripts": {
        "postinstall": "cd electron && npm install",
        "dev": "vite",
        "preview": "vite preview",
        "lint:eslint": "eslint . --max-warnings=0",
        "lint:tsc": "vue-tsc --noEmit",
        "lint": "npm run lint:eslint && npm run lint:tsc",
        "lint:fix": "npm run lint:eslint -- --fix && npm run lint:tsc",
        "test:unit": "vitest run --dom",
        "test:e2e": "npx playwright test --headed --timeout=30000",
        "test:e2e:important": "npx playwright test --timeout=30000 --grep '@important'",
        "test:e2e:headless": "npx playwright test --timeout=30000",
        "web:open": "vite --open",
        "web:release": "vite build",
        "native:build": "node ./scripts/vite_build_for_native.cjs",
        "android:copy": "npm run native:build && npx cap copy android",
        "android:copy:release": "npm run native:build && npx cap copy android",
        "android:open": "npm run android:copy && npx cap open android",
        "ios:copy": "npm run native:build && npx cap copy ios",
        "ios:copy:release": "npm run native:build && npx cap copy ios",
        "ios:open": "npm run ios:copy && npx cap open ios",
        "electron:copy": "npm run native:build && npx cap copy @capacitor-community/electron",
        "electron:copy:dev": "npm run native:build -- --mode development && npx cap copy @capacitor-community/electron",
        "electron:open": "npm run electron:copy:dev && npx cap open @capacitor-community/electron",
        "electron:release": "npm run electron:copy && cd electron && npm run electron:release",
        "electron:release:dev": "npm run electron:copy:dev && cd electron && npm run electron:release",
        "electron:debug": "npm run electron:copy && cd electron && npm run electron:start",
        "prettier:check": "prettier . --check",
        "prettier:fix": "prettier . --write",
        "megashark:local:start": "npm link megashark-lib",
        "megashark:local:stop": "npm unlink megashark-lib --no-save && npm install"
    },
    "dependencies": {
        "@capacitor-community/electron": "github:Scille/capacitor-electron",
        "@capacitor/android": "^7.4.4",
        "@capacitor/app": "^7.1.0",
        "@capacitor/core": "^7.4.4",
        "@capacitor/haptics": "^7.0.2",
        "@capacitor/ios": "^7.4.4",
        "@capacitor/keyboard": "^7.0.3",
        "@capacitor/splash-screen": "^7.0.3",
        "@capacitor/status-bar": "^7.0.3",
        "@ionic/storage": "^4.0.0",
        "@ionic/vue": "^8.7.7",
        "@ionic/vue-router": "^8.7.7",
        "@revolist/vue3-datagrid": "^4.13.4",
        "@sentry/vue": "^9.5.0",
        "@types/luxon": "^3.3.4",
        "@zip.js/zip.js": "^2.8.8",
        "axios": "^1.12.1",
        "detectincognitojs": "^1.5.0",
        "docx-preview": "^0.3.4",
        "file-type": "^20.4.1",
        "fs-extra": "^11.3.0",
        "luxon": "^3.5.0",
        "megashark-lib": "git+https://github.com/Scille/megashark-lib.git#0a3fcd5c02e296c9ff8238fe1c222034decb7ad4",
        "monaco-editor": "^0.52.2",
        "native-file-system-adapter": "^3.0.1",
        "pdfjs-dist": "^5.0.375",
        "qrcode-vue3": "^1.7.1",
        "uuid": "^11.1.0",
        "vite-plugin-static-copy": "^3.1.4",
        "vue": "^3.5.13",
        "vue-i18n": "^11.1.2",
        "vue-router": "^4.5.0",
        "xlsx": "file:vendor/xlsx-0.20.3.tgz"
    },
    "devDependencies": {
        "@capacitor/cli": "^7.4.4",
        "@intlify/eslint-plugin-vue-i18n": "^4.1.0",
        "@ionic/cli": "^7.2.1",
        "@playwright/test": "^1.58.2",
        "@sentry/vite-plugin": "^4.9.0",
        "@types/adm-zip": "^0.5.7",
        "@types/luxon": "^3.3.4",
        "@types/node": "^25.2.2",
        "@types/uuid": "^10.0.0",
        "@types/w3c-image-capture": "^1.0.11",
        "@typescript-eslint/eslint-plugin": "^8.46.2",
        "@typescript-eslint/parser": "^8.46.2",
        "@vitejs/plugin-basic-ssl": "^1.2.0",
        "@vitejs/plugin-vue": "^5.2.4",
        "@vue/eslint-config-typescript": "^14.6.0",
        "@vue/test-utils": "^2.4.6",
        "@vue/typescript": "^1.8.20",
        "@zxcvbn-ts/core": "^3.0.4",
        "@zxcvbn-ts/language-common": "^3.0.4",
        "@zxcvbn-ts/language-en": "^3.0.2",
        "@zxcvbn-ts/language-fr": "^3.0.2",
        "adm-zip": "^0.5.16",
        "dotenv": "^17.2.4",
        "esbuild": "^0.27.3",
        "eslint": "^9.38.0",
        "eslint-config-love": "^133.0.0",
        "eslint-plugin-import": "^2.29.0",
        "eslint-plugin-n": "^17.23.1",
        "eslint-plugin-no-relative-import-paths": "^1.5.3",
        "eslint-plugin-promise": "^7.2.1",
        "eslint-plugin-vue": "^10.5.1",
        "eslint-plugin-vue-scoped-css": "^2.5.1",
        "globals": "^17.3.0",
        "happy-dom": "^20.5.3",
        "jiti": "^2.6.1",
        "prettier": "3.6.2",
        "prettier-plugin-organize-imports": "^4.3.0",
        "sass": "^1.97.3",
        "sass-loader": "^16.0.7",
        "start-server-and-test": "^2.1.3",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.54.0",
        "unique-names-generator": "^4.7.1",
        "vite": "^6.4.1",
        "vite-plugin-top-level-await": "^1.5.0",
        "vitest": "3.0.5",
        "vue-eslint-parser": "^10.2.0",
        "vue-tsc": "^3.2.4"
    }
}
