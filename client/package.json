{
    "name": "client-vite",
    "private": true,
    "version": "3.0.0-rc.1+dev",
    "type": "module",
    "license": "BUSL-1.1",
    "scripts": {
        "postinstall": "cd electron && npm install",
        "dev": "vite",
        "preview": "vite preview",
        "lint": "eslint . --max-warnings=0 && vue-tsc --noEmit",
        "test:unit": "vitest run --dom",
        "cy:e2e:headless": "cypress run --e2e",
        "cy:e2e": "cypress run --e2e --headed",
        "pw:e2e": "npx playwright test --headed --reporter=null --timeout=30000",
        "pw:e2e:headless": "npx playwright test --reporter=null --timeout=30000",
        "test:e2e": "start-server-and-test dev http://localhost:8080 cy:e2e",
        "test:e2e:headless": "start-server-and-test dev http://localhost:8080 cy:e2e:headless",
        "web:open": "vite --open",
        "web:release": "npm run lint && vite build",
        "native:build": "node ./scripts/vite_build_for_native.cjs",
        "android:copy": "npm run native:build && npx cap copy android",
        "android:copy:release": "npm run native:build && npx cap copy android",
        "android:open": "npm run android:copy && npx cap open android",
        "ios:copy": "npm run native:build && npx cap copy ios",
        "ios:copy:release": "npm run native:build && npx cap copy ios",
        "ios:open": "npm run ios:copy && npx cap open ios",
        "electron:copy": "npm run native:build && npx cap copy @capacitor-community/electron",
        "electron:open": "npm run electron:copy && npx cap open @capacitor-community/electron",
        "electron:release": "npm run electron:copy && cd electron && npm run electron:release",
        "electron:debug": "npm run electron:copy && cd electron && npm run electron:start",
        "prettier:check": "prettier . --check",
        "prettier:fix": "prettier . --write",
        "megashark:local:start": "npm link megashark-lib",
        "megashark:local:stop": "npm unlink megashark-lib --no-save && npm install"
    },
    "dependencies": {
        "@capacitor-community/electron": "^5.0.1",
        "@capacitor/android": "^5.6.0",
        "@capacitor/app": "^5.0.6",
        "@capacitor/core": "^5.2.1",
        "@capacitor/haptics": "^5.0.6",
        "@capacitor/ios": "^5.6.0",
        "@capacitor/keyboard": "^5.0.7",
        "@capacitor/splash-screen": "^5.0.6",
        "@capacitor/status-bar": "^5.0.6",
        "@ionic/storage": "^4.0.0",
        "@ionic/vue": "^7.6.4",
        "@ionic/vue-router": "^7.6.4",
        "@sentry/vue": "^8.27.0",
        "@types/luxon": "^3.3.4",
        "axios": "^1.7.7",
        "luxon": "^3.5.0",
        "megashark-lib": "git+https://github.com/Scille/megashark-lib.git#06df6c588fa6cce63a8dec6ff4a668313ae039e9",
        "qrcode-vue3": "^1.6.8",
        "uuid": "^10.0.0",
        "vue": "^3.4.38",
        "vue-i18n": "^9.14.0",
        "vue-router": "^4.4.3"
    },
    "devDependencies": {
        "@capacitor/cli": "^5.6.0",
        "@intlify/eslint-plugin-vue-i18n": "^3.0.0",
        "@ionic/cli": "^7.2.0",
        "@playwright/test": "^1.46.1",
        "@sentry/vite-plugin": "^2.22.3",
        "@types/luxon": "^3.3.4",
        "@types/node": "^22.5.2",
        "@types/uuid": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^8.3.0",
        "@typescript-eslint/parser": "^8.3.0",
        "@vitejs/plugin-vue": "^5.1.3",
        "@vue/eslint-config-typescript": "^13.0.0",
        "@vue/test-utils": "^2.4.6",
        "@vue/typescript": "^1.8.20",
        "@zxcvbn-ts/core": "^3.0.4",
        "@zxcvbn-ts/language-common": "^3.0.4",
        "@zxcvbn-ts/language-en": "^3.0.2",
        "@zxcvbn-ts/language-fr": "^3.0.2",
        "cypress": "^13.14.1",
        "cypress-file-upload": "^5.0.8",
        "cypress-real-events": "^1.13.0",
        "cypress-vite": "^1.4.2",
        "dotenv": "^16.4.5",
        "electron-builder-notarize": "^1.5.2",
        "esbuild": "^0.23.1",
        "eslint": "^9.9.1",
        "eslint-config-standard-with-typescript": "^43.0.1",
        "eslint-plugin-cypress": "^3.5.0",
        "eslint-plugin-import": "^2.29.0",
        "eslint-plugin-n": "^17.10.2",
        "eslint-plugin-no-relative-import-paths": "^1.5.5",
        "eslint-plugin-promise": "^7.1.0",
        "eslint-plugin-vue": "^9.27.0",
        "eslint-plugin-vue-scoped-css": "^2.8.1",
        "happy-dom": "^15.7.3",
        "prettier": "3.3.3",
        "prettier-plugin-organize-imports": "^4.0.0",
        "sass": "^1.77.8",
        "sass-loader": "^16.0.1",
        "start-server-and-test": "^2.0.5",
        "typescript": "^5.5.4",
        "unique-names-generator": "^4.7.1",
        "vite": "^5.4.2",
        "vite-plugin-top-level-await": "^1.4.4",
        "vitest": "^2.0.5",
        "vue-tsc": "^2.1.4"
    }
}
