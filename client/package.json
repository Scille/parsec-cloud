{
    "name": "client-vite",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "postinstall": "cd electron && npm install",
        "dev": "vite",
        "preview": "vite preview",
        "lint": "eslint . && vue-tsc --noEmit",
        "test:unit": "vitest run --dom",
        "cy:e2e:headless": "cypress run --e2e",
        "cy:e2e": "cypress run --e2e --headed",
        "test:e2e": "start-server-and-test dev http://localhost:8080 cy:e2e",
        "test:e2e:headless": "start-server-and-test dev http://localhost:8080 cy:e2e:headless",
        "web:open": "vite --open",
        "web:release": "npm run lint && vite build",
        "native:build": "node ./scripts/vite_build_for_native.cjs",
        "android:copy": "npm run native:build && npx cap copy android",
        "android:copy:release": "npm run native:build && npx cap copy android",
        "android:open": "npm run android:copy && npx cap open android",
        "ios:copy": "npm run native:build && npx cap copy ios",
        "ios:copy:release": "npm run native:build && npx cap copy ios",
        "ios:open": "npm run ios:copy && npx cap open ios",
        "electron:copy": "npm run native:build && npx cap copy @capacitor-community/electron",
        "electron:open": "npm run electron:copy && npx cap open @capacitor-community/electron",
        "electron:release": "npm run electron:copy && cd electron && npm run electron:release"
    },
    "dependencies": {
        "@capacitor-community/electron": "^4.1.2",
        "@capacitor/android": "^5.2.1",
        "@capacitor/app": "^5.0.6",
        "@capacitor/core": "^5.2.1",
        "@capacitor/haptics": "^5.0.6",
        "@capacitor/ios": "^5.2.1",
        "@capacitor/keyboard": "^5.0.6",
        "@capacitor/splash-screen": "^5.0.6",
        "@capacitor/status-bar": "^5.0.6",
        "@ionic/storage": "^4.0.0",
        "@ionic/vue": "^7.1.3",
        "@ionic/vue-router": "^7.1.3",
        "@types/luxon": "^3.2.0",
        "luxon": "^3.3.0",
        "uuid": "^9.0.0",
        "vue": "^3.2.45",
        "vue-i18n": "^9.2.2",
        "vue-router": "^4.2.4"
    },
    "devDependencies": {
        "@capacitor/cli": "^5.2.1",
        "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
        "@ionic/cli": "^7.1.1",
        "@types/luxon": "^3.2.0",
        "@types/node": "^20.4.2",
        "@types/uuid": "^9.0.2",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "@vitejs/plugin-vue": "^4.2.3",
        "@vue/eslint-config-typescript": "^10.0.0",
        "@vue/test-utils": "^2.4.0",
        "@zxcvbn-ts/core": "^3.0.2",
        "@zxcvbn-ts/language-common": "^3.0.3",
        "@zxcvbn-ts/language-en": "^3.0.1",
        "@zxcvbn-ts/language-fr": "^3.0.1",
        "cypress": "^12.17.1",
        "cypress-vite": "^1.4.2",
        "esbuild": "^0.18.13",
        "eslint": "^8.45.0",
        "eslint-config-standard-with-typescript": "^36.1.0",
        "eslint-plugin-cypress": "^2.13.3",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-n": "^16.0.1",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-vue": "^8.0.1",
        "eslint-plugin-vue-scoped-css": "^2.5.0",
        "happy-dom": "^10.3.2",
        "sass": "^1.63.6",
        "sass-loader": "^13.3.2",
        "start-server-and-test": "^2.0.0",
        "typescript": "^5.1.6",
        "unique-names-generator": "^4.7.1",
        "vite": "^4.4.4",
        "vite-plugin-top-level-await": "^1.3.1",
        "vitest": "^0.33.0",
        "vue-tsc": "^1.8.5"
    }
}
