{
    "label": "LocalDevice",
    "type": "local_device",
    "other_fields": [
        {
            // cspell:disable-next-line
            // URL has format `parsec3://parsec.example.com/MyOrg?p=xCBs8zpdIwovR8EdliVVo2vUOmtumnfsI6Fdndjm0WconA`.
            // Note legacy `parsec://parsec.example.com/MyOrg?rvk=7NFDS4VQLP3XPCMTSEN34ZOXKGGIMTY2W2JI2SPIHB2P3M6K4YWAssss`
            // format is also supported here for backwards compatibility.
            "name": "organization_addr",
            "type": "ParsecOrganizationAddr"
        },
        {
            "name": "user_id",
            "type": "UserID"
        },
        {
            "name": "device_id",
            "type": "DeviceID"
        },
        {
            "name": "device_label",
            "type": "DeviceLabel"
        },
        {
            "name": "human_handle",
            "type": "HumanHandle"
        },
        {
            "name": "signing_key",
            "type": "SigningKey"
        },
        {
            "name": "private_key",
            "type": "PrivateKey"
        },
        {
            // Note user profile can change by uploading additional `UserUpdateCertificate`.
            // Hence this field only contains the initial profile the user had when it
            // was enrolled.
            "name": "initial_profile",
            "type": "UserProfile"
        },
        {
            "name": "user_realm_id",
            "type": "VlobID"
        },
        {
            "name": "user_realm_key",
            "type": "SecretKey"
        },
        {
            "name": "local_symkey",
            "type": "SecretKey"
        }
    ]
}
