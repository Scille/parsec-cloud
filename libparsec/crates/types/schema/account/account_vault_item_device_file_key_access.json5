{
    "label": "AccountVaultItemDeviceFileKeyAccess",
    "type": "account_vault_item_device_file_key_access",
    // Introduced in Parsec 3.5.0
    "introduced_in_revision": 350,
    "other_fields": [
        {
            "name": "organization_id",
            "type": "OrganizationID"
        },
        // User ID is not provided here since it is not relevant:
        // this item is only used by clients looking to decrypt a given device.
        {
            "name": "device_id",
            "type": "DeviceID"
        },
        {
            // `DeviceFileAccountVaultCiphertextKey` encrypted by the vault key.
            // `DeviceFileAccountVaultCiphertextKey` itself contains a key used
            // to decrypt the `LocalDevice` stored in a `DeviceFileAccountVault`.
            "name": "encrypted_data",
            "type": "Bytes"
        }
    ]
}
